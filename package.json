{
  "name": "dude-claude-plugin",
  "version": "2026.2.11",
  "description": "Ultra-minimal RAG and cross-project memory for Claude CLI",
  "type": "module",
  "bin": {
    "dude-claude": "bin/dude-claude.js"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "node bin/dude-claude.js mcp",
    "serve": "node bin/dude-claude.js serve",
    "prepublishOnly": "node scripts/sync-plugin-version.cjs",
    "release": "npm version patch && git push && git push --tags && npm publish",
    "release:minor": "npm version minor && git push && git push --tags && npm publish",
    "release:major": "npm version major && git push && git push --tags && npm publish"
  },
  "keywords": [
    "claude",
    "claude-code",
    "plugin",
    "mcp",
    "rag",
    "memory",
    "knowledge-base",
    "vector-search"
  ],
  "author": "Fingerskier",
  "repository": {
    "type": "git",
    "url": "https://github.com/fingerskier/dude-claude-plugin.git"
  },
  "homepage": "https://github.com/fingerskier/dude-claude-plugin#readme",
  "bugs": {
    "url": "https://github.com/fingerskier/dude-claude-plugin/issues"
  },
  "files": [
    "bin/",
    "src/",
    "hooks/",
    "web/",
    ".claude-plugin/",
    ".mcp.json",
    "hooks.json",
    "mcp-servers.json"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "zod": "^3.24.2",
    "better-sqlite3": "^11.8.1",
    "sqlite-vec": "^0.1.6",
    "@huggingface/transformers": "^3.4.1"
  },
  "license": "MIT"
}
